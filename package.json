{
  "name": "community-organiser-quasar",
  "version": "0.0.1",
  "description": "Organise small organisations like your child's kindergarten society, small accounting, less stressfull automated reminders, family life or just own matters",
  "productName": "Community Organiser",
  "author": "Jacek Miszczuk",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "electron": "quasar dev -m electron",
    "android": "quasar build -m capacitor -T android",
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@quasar/extras": "^1.16.4",
    "date-fns": "^4.1.0",
    "quasar": "^2.16.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.22",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.0",
    "@electron/packager": "^18.4.4",
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
